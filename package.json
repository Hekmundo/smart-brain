    "bcrypt-nodejs": "^0.0.3",
    "clarifai": "^2.9.1",
    "compression": "^1.7.4",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "knex": "^0.95.6",
    "pg": "^8.6.0",
    "redux-persist": "^6.0.0"
  }
}
